<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.program.temasjan" name="Leia Themes Jan Wizard" version="2.0" provider-name="HKP">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  
  <extension point="xbmc.addon.repository" name="walterbishop2019">
        <info>https://raw.githubusercontent.com/walterbishop2019/walterbishop/master/zips/addons.xml</info>
        <checksum>https://raw.githubusercontent.com/walterbishop2019/walterbishop/master/zips/addons.xml.md5</checksum>
        <datadir zip="true">https://raw.githubusercontent.com/walterbishop2019/walterbishop/master/zips/</datadir>
    </extension>
  
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Leia Themes Jan Wizard</summary>
    <description lang="en">Leia Themes Jan Wizard</description>
    <platform>all</platform>
  </extension>
</addon>